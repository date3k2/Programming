import bisect
import sys
input = sys.stdin.readline
def lengthOfLIS(nums: list[int]) -> int:
    l = []
    for num in nums:
        x = bisect.bisect_left(l,num)
        if x==len(l):
            l.append(num)
        else:
            l[x]=num
    return len(l)
n = int(input())
nums = list(map(int,input().split()))
print(lengthOfLIS(nums))